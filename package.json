    "cli.js"
  ],
  "bin": {
    "swagger-cleaner": "./cli.js"
  },
  "repository": "git@github.com:dhlavaty/swagger-cleaner.git",
  "author": "Dusan Hlavaty <dhlavaty@gmail.com>",
  "engines": {
    "node": ">= 8"
  }
