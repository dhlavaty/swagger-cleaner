{
  "name": "swagger-cleaner",
  "version": "0.0.5",
  "license": "MIT",
  "devDependencies": {
    "@zeit/ncc": "^0.22.3",
    "json-stable-stringify": "^1.0.1",
    "node-fetch": "^3.1.1"
  },
  "keywords": [
    "json",
    "swagger",
    "diff",
    "cleaner",
    "nodejs",
    "comparison",
    "fetch",
    "cli"
